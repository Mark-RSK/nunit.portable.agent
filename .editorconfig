# EditorConfig helps developers define and
# maintain consistent coding styles between
# different editors and IDEs

# http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = crlf
indent_style = space
indent_size = 2
tab_width = 4
trim_trailing_whitespace = true

[*.cs]
indent_style = space
indent_size = 4